{
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        { "key": "Content-Type", "value": "text/plain" },
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    },
    {
      "source": "/(.*)\\.(xml|txt|ico|json|js|css|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|eot|html)$",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" },
        { "key": "Content-Type", "value": "text/html; charset=utf-8" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/merge-large-pdf-files", "destination": "/merge-large-pdf-files.html" },
    { "source": "/combine-pdf-any-device", "destination": "/combine-pdf-any-device.html" },
    { "source": "/how-to-merge-pdf-files", "destination": "/how-to-merge-pdf-files.html" },
    { "source": "/merge-pdf-for-business", "destination": "/merge-pdf-for-business.html" },
    { "source": "/merge-pdf-online-free", "destination": "/merge-pdf-online-free.html" },
    { "source": "/compress-pdf-file-size", "destination": "/compress-pdf-file-size.html" },
    { "source": "/reduce-pdf-size-for-email", "destination": "/reduce-pdf-size-for-email.html" },
    { "source": "/compress-pdf-for-web", "destination": "/compress-pdf-for-web.html" },
    { "source": "/compress-scanned-pdf-files", "destination": "/compress-scanned-pdf-files.html" },
    { "source": "/compress-pdf-without-losing-quality", "destination": "/compress-pdf-without-losing-quality.html" },
    { "source": "/split-pdf-online-guide", "destination": "/split-pdf-online-guide.html" },
    { "source": "/split-large-pdf-files", "destination": "/split-large-pdf-files.html" },
    { "source": "/split-pdf-by-pages-or-chapters", "destination": "/split-pdf-by-pages-or-chapters.html" },
    { "source": "/split-pdf-for-email", "destination": "/split-pdf-for-email.html" },
    { "source": "/split-scanned-and-legal-pdf-documents", "destination": "/split-scanned-and-legal-pdf-documents.html" },
    { "source": "/convert-pdf-to-jpg-online", "destination": "/convert-pdf-to-jpg-online.html" },
    { "source": "/pdf-to-jpg-multiple-pages", "destination": "/pdf-to-jpg-multiple-pages.html" },
    { "source": "/pdf-to-jpg-for-web-and-social", "destination": "/pdf-to-jpg-for-web-and-social.html" },
    { "source": "/extract-images-from-pdf-to-jpg", "destination": "/extract-images-from-pdf-to-jpg.html" },
    { "source": "/pdf-to-jpg-from-scanned-documents", "destination": "/pdf-to-jpg-from-scanned-documents.html" },
    { "source": "/convert-jpg-to-pdf-online", "destination": "/convert-jpg-to-pdf-online.html" },
    { "source": "/jpg-to-pdf-multiple-images", "destination": "/jpg-to-pdf-multiple-images.html" },
    { "source": "/jpg-to-pdf-scanned-documents", "destination": "/jpg-to-pdf-scanned-documents.html" },
    { "source": "/jpg-to-pdf-for-email-and-sharing", "destination": "/jpg-to-pdf-for-email-and-sharing.html" },
    { "source": "/organize-photos-and-notes-with-jpg-to-pdf", "destination": "/organize-photos-and-notes-with-jpg-to-pdf.html" },
    {
      "source": "/((?!static/.*|sitemap\\.xml|robots\\.txt|.*\\.(?:png|jpg|jpeg|gif|svg|webp|ico|json|js|css|woff|woff2|ttf|eot|html)$).*)",
      "destination": "/index.html"
    }
  ]
}
